{
  "name": "node-ts-practice",
  "version": "0.0.0",
  "description": "node.js typescript backend practice project for after a while of not using node and typescript",
  "type": "module",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@types/eslint__js": "~8.42",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/express": "^5.0.0",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.5",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "~9.10",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.9",
    "prettier": "~3.3",
    "rimraf": "~6.0",
    "ts-api-utils": "~1.3",
    "typescript": "~5.5",
    "typescript-eslint": "~8.5",
    "vitest": "~2.1"
  },
  "scripts": {
    "start": "node build/src/app.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "test": "vitest run unit",
    "test:coverage": "vitest run unit --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest unit"
  },
  "author": "Asher Clawson",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^10.0.1",
    "mongoose": "^8.7.0",
    "tslib": "~2.7"
  },
  "volta": {
    "node": "20.17.0"
  }
}
